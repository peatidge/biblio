@{
    //TODO:b 443. Add Member Home Index View
    ViewData["Title"] = "RE:MEMBER HOME";
}
<div id="lemur-progress" class="position-absolute start-50 translate-middle-x overflow-hidden war-p"
     style="top:60px;z-index:2;font-size:3rem">
</div>
<h1 class="war-p mt-1 schrodinger" style="font-size:1.4rem">@ViewData["Title"]</h1>
<p class="war-p schrodinger" style="font-size:0.6rem">Chaollibration Instructions: Drag & Drop Axelotl into Tesseract</p>
<div class="schrodinger d-flex justify-content-between">
    <img src="/images/possibility-s.webp"
         class="rounded w-25 opacity-25 draggable"
         id="Possibility"
         alt="Possibility"
         title="Drag & Drop me into Tesseract"
         style="cursor:grab"
         draggable="true" />

    <img src="/images/ytilibissop-s.webp"
         class="rounded img-fluid w-25 opacity-25"
         id="Ytilibissop"
         alt="Ytilibissop"
         dropzone="true"
         title="Drop Possibility on Me" />
</div>
<div id="lemurs" class="position-absolute top-0 start-50 translate-middle-x mh-100 overflow-hidden war-p" style="max-width:95vw;font-size:0.8rem;display:none;">
    <pre></pre>
</div>
<div id="lemurs-over-and-out" class="position-absolute top-0 start-0 w-100 h-100 z-n1" style="display:none;"></div>
<div class="mt-3">
    <div id="axolert" class="alert  alert-info  fade show" style="display:none"></div>
    <div class="alert alert-warning p-2  alert-dismissible fade show schrodinger pe-4" role="alert" style="font-size:0.7rem" id="photo-warning">
        <span class="fw-bolder">Warning: Bright Fast Flashing Content</span><br />
        <span>Attention: Sensitive Viewers</span>
        <span>This content contains bright, fast flashing images that may potentially trigger seizures for people with photosensitive epilepsy. Viewer discretion is advised. If you or someone you know is sensitive to flashing lights, please proceed with caution or consider skipping this content.</span>
        <span> Thank you for your understanding and stay safe!</span>
        <button type="button" class="btn-close p-2" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
</div>
<span style="display:none" id="axolotransmission">
    <img src="/images/possibility-s.webp" class="rounded-2 opacity-25 draggable axoclone" style="width:50px" alt="Possibility" title="Drop Me" />
</span>
@section Scripts {
    <script src="https://cdnjs.cloudflare.com/ajax/libs/hammer.js/2.0.8/hammer.min.js"
            integrity="sha512-UXumZrZNiOwnTcZSHLOfcTs0aos2MzBWHXOHOuB0J/R44QB0dwY5JgfbvljXcklVf65Gc4El6RjZ+lnwd2az2g=="
            crossorigin="anonymous"
            referrerpolicy="no-referrer">
    </script>
    <environment include="Development">
        <script src="/js/lemur.min.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="https://quiz-engin-cdn-dtbyasb5ekdvc6h5.z01.azurefd.net/js/lemur.min.js"></script>
    </environment>
}